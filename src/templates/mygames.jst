<h1> My Games</h1>
<div class="row">
    <%for (var i=0;i < games.length;i++){%>
    <div class="card">
        <div class="card-header">
            <h1><%=games[i].Name%></h1>
            <span><%=statusIcons[games[i].Status]%></span>
        </div>
        <div class="card-body">
            <ul class="list-group list-group-flush">
                <li class="list-group-item">Genre(s): <%=games[i].Genre%>
                </li>
                <li class="list-group-item">Platform(s): <%=games[i].Platform%>
                </li>
                <li class="list-group-item">Format: <%=games[i].Format%></li>
            </ul>
            <input type='button' class="btn btn-primary" value='Edit Item'>
        </div>
    </div>
    <%}%>
</div>